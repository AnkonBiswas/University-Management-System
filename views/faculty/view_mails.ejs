<!DOCTYPE html>
<html>
<head>
	<title>VIEW MAILS</title>
</head>
<body>
	<h1>VIEW MAILS </h1>

	<a href="/faculty">PROFILE</a> |
	<a href="/faculty/mail">MAILS</a> | 
	<a href="/logout">LOGOUT</a> | 

<br><br>

<form method="post">
        <table border="1">
                <tr>
                    
                    <td>MAIL ID</td>
                    <td>STUDENT ID</td>
                    <td>MAIL</td>
                </tr>
        
            <% for(var i=0; i< user.length; i++){ %>
                <tr>
                    <td><%= user[i].mail_id %></td>
                    <td><%= user[i].student_id %></td>
                    <td><%= user[i].mail %></td>
                </tr>
        
            <% } %>
            </table>
</form>
</body>
</html>